<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Contact</name>
  <author>A. Kempkens</author>
  <version>1.8</version>
  <description>Definition for the core contact component</description>
  <reference>
  	<table name="contact_details">
  		<field type="referenceid" name="id" translate="0" jgtranslate="0">ID</field>
  		<field type="titletext" name="name" translate="1" jgtranslate="1">Name</field>
  		<field type="text" name="con_position" translate="1" jgtranslate="1">Position</field>
  		<field type="text" name="email_to" translate="1" jgtranslate="0">Email Address</field>
  		<field type="text" name="address" translate="1" jgtranslate="1">Street Address</field>
  		<field type="text" name="suburb" translate="1" jgtranslate="1">Town/Suburb</field>
  		<field type="text" name="state" translate="1" jgtranslate="1">State/County</field>
  		<field type="text" name="country" translate="1" jgtranslate="1">Country</field>
  		<field type="text" name="postcode" translate="1" jgtranslate="1">Postcode</field>
  		<field type="text" name="telephone" translate="1" jgtranslate="1">Telefon</field>
  		<field type="text" name="fax" translate="1" jgtranslate="1">Fax</field>
  		<field type="textarea" name="misc" translate="1" jgtranslate="1">Miscellaneous Info</field>
  		<field type="textarea" name="params" translate="1" jgtranslate="0">Parameters</field>  		
  	</table>
  </reference>
</joomfish>