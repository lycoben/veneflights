		<param name="showPathway" type="radio" default="1" label="Show Pathway" description="Definition whether to show pathway or not">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="enableFontresizer" type="radio" default="1" label="Enable Font Size Selector" description="Definition whether to show 3 buttons to change font size on toolbar or not">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="enableIconLinks" type="radio" default="1" label="Enable Auto Icon links" description="Definition whether to enable auto icon link or not. Icons can still be assigned to links by class attribute even if this option is turned off">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="enablePNGfix" type="radio" default="1" label="Enable PNG Fix for IE6" description="Definition whether to enable PNG fix feature for IE6 or not. This parameter should be turned off only when there are incompatibility issues">
			<option value="0">No</option>
			<option value="1">Yes</option>
